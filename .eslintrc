{
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": ["@typescript-eslint"],
    "env": {
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      // Here can be rules added: Google eslint.org & rules
    },
    "ignorePatterns": ["dist/", "node_modules/"],
    "overrides": [
      {
        "files": ["src/**/*.ts"],
        "excludedFiles": "*.test.ts"
      }
    ]
  }
  